{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Deno",
			"type": "node",
			"request": "launch",
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "deno",
			"runtimeArgs": [
				"run",
        "--inspect",
				"--allow-read",
				"--allow-net",
				"--allow-write",
				"--lock=labs/lock.json",
				"--lock-write",
        "--unstable",
				"--allow-env",
				"-A",
				"labs/index.ts"
			],
			"port": 9229
		}
	]
}
